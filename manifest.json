{
    "manifest_version": 2,
    "name": "DDG Position",
    "version": "1.0",

    "description": "Places a marker with the position of a google result link in DDG result",

    "content_scripts": [
        {
            "matches": ["*://*.google.com/search?*"],
            "js": ["main.js"]
        }
    ],

    "permissions": [
        "*://duckduckgo.com/html/*"
    ]
}